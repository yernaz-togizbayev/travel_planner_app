<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="370dp"
    android:background="#DEF0F7">

    <androidx.cardview.widget.CardView
        android:id="@+id/travel_destinantion_card"
        android:layout_width="413dp"
        android:layout_height="358dp"
        android:background="#AFA9A9"
        android:backgroundTint="#8AD8DC"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.593">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            tools:layout_editor_absoluteX="24dp"
            tools:layout_editor_absoluteY="-1dp">

            <ImageView
                android:id="@+id/logoImageView"
                android:layout_width="0dp"
                android:layout_height="82dp"
                android:contentDescription="Logo"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/symbolImageView"
                app:layout_constraintTop_toTopOf="parent"
                app:srcCompat="@drawable/austrian_logo" />

            <ImageView
                android:id="@+id/symbolImageView"
                android:layout_width="83dp"
                android:layout_height="79dp"
                android:layout_marginTop="2dp"
                android:contentDescription="travelSymbol"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:srcCompat="@drawable/ic_plane" />

            <ImageView
                android:id="@+id/imageView9"
                android:layout_width="46dp"
                android:layout_height="37dp"
                android:layout_marginStart="2dp"
                android:layout_marginTop="4dp"
                android:contentDescription="location"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/symbolImageView"
                app:srcCompat="@drawable/ic_location" />

            <TextView
                android:id="@+id/destinationTextView"
                android:layout_width="351dp"
                android:layout_height="47dp"
                android:layout_marginStart="3dp"
                android:fontFamily="sans-serif-medium"
                android:gravity="center|start"
                android:text="Tokyo Airport"
                android:textSize="20sp"
                app:layout_constraintBottom_toBottomOf="@+id/imageView9"
                app:layout_constraintStart_toEndOf="@+id/imageView9"
                app:layout_constraintTop_toTopOf="@+id/imageView9" />

            <ImageView
                android:id="@+id/imageView10"
                android:layout_width="46dp"
                android:layout_height="41dp"
                android:layout_marginStart="4dp"
                android:layout_marginTop="4dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/imageView9"
                app:srcCompat="@drawable/ic_depature" />

            <TextView
                android:id="@+id/depatureDateTextView"
                android:layout_width="107dp"
                android:layout_height="29dp"
                android:layout_marginStart="12dp"
                android:layout_marginTop="44dp"
                android:fontFamily="sans-serif-medium"
                android:text="dd.MM.yyyy"
                android:textAlignment="center"
                android:textSize="18sp"
                app:layout_constraintStart_toEndOf="@+id/imageView12"
                app:layout_constraintTop_toBottomOf="@+id/imageView9" />

            <TextView
                android:id="@+id/depatureTimeTextView"
                android:layout_width="106dp"
                android:layout_height="30dp"
                android:layout_marginTop="8dp"
                android:fontFamily="sans-serif-medium"
                android:text="10:10"
                android:textAlignment="center"
                android:textSize="18sp"
                app:layout_constraintEnd_toEndOf="@+id/depatureDateTextView"
                app:layout_constraintHorizontal_bias="1.0"
                app:layout_constraintStart_toStartOf="@+id/depatureDateTextView"
                app:layout_constraintTop_toBottomOf="@+id/depatureDateTextView" />

            <ImageView
                android:id="@+id/imageView11"
                android:layout_width="37dp"
                android:layout_height="29dp"
                android:layout_marginStart="4dp"
                android:layout_marginTop="80dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/imageView9"
                app:srcCompat="@drawable/ic_time" />

            <ImageView
                android:id="@+id/imageView12"
                android:layout_width="37dp"
                android:layout_height="28dp"
                android:layout_marginStart="4dp"
                android:layout_marginTop="40dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/destinationTextView"
                app:srcCompat="@drawable/calender_icon" />

            <ImageView
                android:id="@+id/imageView21"
                android:layout_width="43dp"
                android:layout_height="40dp"
                android:layout_marginStart="164dp"
                app:layout_constraintStart_toEndOf="@+id/imageView10"
                app:layout_constraintTop_toBottomOf="@+id/destinationTextView"
                app:srcCompat="@drawable/ic_arrival" />

            <View
                android:id="@+id/divider12"
                android:layout_width="1dp"
                android:layout_height="0dp"
                android:layout_marginStart="40dp"
                android:layout_marginBottom="112dp"
                android:background="?android:attr/listDivider"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toEndOf="@+id/depatureDateTextView"
                app:layout_constraintTop_toBottomOf="@+id/destinationTextView"
                app:layout_constraintVertical_bias="0.0" />

            <TextView
                android:id="@+id/arrivalDateTextView"
                android:layout_width="107dp"
                android:layout_height="29dp"
                android:layout_marginStart="12dp"
                android:layout_marginTop="40dp"
                android:fontFamily="sans-serif-medium"
                android:text="dd.MM.yyyy"
                android:textAlignment="center"
                android:textSize="18sp"
                app:layout_constraintStart_toEndOf="@+id/imageView19"
                app:layout_constraintTop_toBottomOf="@+id/destinationTextView" />

            <ImageView
                android:id="@+id/imageView19"
                android:layout_width="37dp"
                android:layout_height="28dp"
                android:layout_marginStart="4dp"
                android:layout_marginTop="40dp"
                app:layout_constraintStart_toStartOf="@+id/imageView21"
                app:layout_constraintTop_toBottomOf="@+id/destinationTextView"
                app:srcCompat="@drawable/calender_icon" />

            <ImageView
                android:id="@+id/imageView20"
                android:layout_width="38dp"
                android:layout_height="30dp"
                android:layout_marginTop="8dp"
                app:layout_constraintStart_toStartOf="@+id/imageView19"
                app:layout_constraintTop_toBottomOf="@+id/imageView19"
                app:srcCompat="@drawable/ic_time" />

            <TextView
                android:id="@+id/arrivalTimeTextView"
                android:layout_width="106dp"
                android:layout_height="30dp"
                android:layout_marginStart="12dp"
                android:layout_marginTop="12dp"
                android:fontFamily="sans-serif-medium"
                android:text="10:10"
                android:textAlignment="center"
                android:textSize="18sp"
                app:layout_constraintStart_toEndOf="@+id/imageView20"
                app:layout_constraintTop_toBottomOf="@+id/arrivalDateTextView" />

            <ImageView
                android:id="@+id/imageView22"
                android:layout_width="34dp"
                android:layout_height="40dp"
                android:layout_marginStart="4dp"
                android:layout_marginTop="24dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/imageView11"
                app:srcCompat="@drawable/baseline_euro_symbol_24" />

            <TextView
                android:id="@+id/costTextView"
                android:layout_width="109dp"
                android:layout_height="44dp"
                android:layout_marginStart="8dp"
                android:fontFamily="sans-serif-medium"
                android:text="1000.00â‚¬"
                android:textAlignment="center"
                android:textSize="20sp"
                app:layout_constraintBottom_toBottomOf="@+id/imageView22"
                app:layout_constraintStart_toEndOf="@+id/imageView22"
                app:layout_constraintTop_toTopOf="@+id/imageView22"
                app:layout_constraintVertical_bias="0.0" />

            <Button
                android:id="@+id/bookTravelButton"
                android:layout_width="196dp"
                android:layout_height="86dp"
                android:layout_marginStart="44dp"
                android:layout_marginTop="28dp"
                android:backgroundTint="#155FD4"
                android:text="Book Travel"
                android:textSize="20sp"
                android:textStyle="bold"
                app:layout_constraintStart_toEndOf="@+id/costTextView"
                app:layout_constraintTop_toBottomOf="@+id/arrivalTimeTextView" />

            <ImageView
                android:id="@+id/imageView13"
                android:layout_width="34dp"
                android:layout_height="34dp"
                android:layout_marginStart="4dp"
                android:layout_marginTop="20dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/imageView22"
                app:srcCompat="@drawable/ic_time" />

            <TextView
                android:id="@+id/travelDurationTextView"
                android:layout_width="97dp"
                android:layout_height="33dp"
                android:layout_marginStart="8dp"
                android:fontFamily="sans-serif-medium"
                android:text="16h"
                android:textAlignment="center"
                android:textSize="18sp"
                app:layout_constraintStart_toEndOf="@+id/imageView13"
                app:layout_constraintTop_toTopOf="@+id/imageView13" />

            <View
                android:id="@+id/divider10"
                android:layout_width="411dp"
                android:layout_height="1dp"
                android:layout_marginBottom="8dp"
                android:background="?android:attr/listDivider"
                app:layout_constraintBottom_toTopOf="@+id/costTextView"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent" />

            <View
                android:id="@+id/divider11"
                android:layout_width="411dp"
                android:layout_height="1dp"
                android:layout_marginTop="-3dp"
                android:background="?android:attr/listDivider"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="1.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/destinationTextView" />

            <TextView
                android:id="@+id/textView25"
                android:layout_width="132dp"
                android:layout_height="37dp"
                android:layout_marginStart="1dp"
                android:fontFamily="sans-serif-medium"
                android:text="Outward Journey"
                android:textAlignment="center"
                android:textSize="16sp"
                android:textStyle="bold"
                app:layout_constraintStart_toEndOf="@+id/imageView10"
                app:layout_constraintTop_toBottomOf="@+id/destinationTextView" />

            <TextView
                android:id="@+id/textView26"
                android:layout_width="149dp"
                android:layout_height="36dp"
                android:layout_marginStart="4dp"
                android:layout_marginTop="4dp"
                android:fontFamily="sans-serif-medium"
                android:text="Return Journey"
                android:textAlignment="center"
                android:textSize="16sp"
                android:textStyle="bold"
                app:layout_constraintStart_toEndOf="@+id/imageView21"
                app:layout_constraintTop_toBottomOf="@+id/destinationTextView" />

        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.cardview.widget.CardView>
</androidx.constraintlayout.widget.ConstraintLayout>